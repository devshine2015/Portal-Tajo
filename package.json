    "moment": "2.13.0",
    "node-polyglot": "2.2.2",
    "node-uuid": "1.4.8",
    "prop-types": "15.6.0",
    "query-string": "5.0.0",
    "ramda": "0.24.1",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-native-storage": "0.2.2",
    "react-redux": "5.0.6",
