{
  "name": "drvr.co",
  "version": "1.4.434",
  "description": "common code base for DRVR portals",
  "repository": {
    "type": "git",
    "url": "https://github.com/drvrteam/portal-tajo.git"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./build ./builds",
    "precz": "npm test",
    "cz": "git-cz",
    "build:scc": "cross-env NODE_ENV=production DRVR_PROJECT=scc webpack --config webpack/webpack.prod.js --color",
    "build:demo": "cross-env NODE_ENV=production DRVR_PROJECT=demo webpack --config webpack/webpack.prod.js --color",
    "build:dealer": "cross-env NODE_ENV=production DRVR_PROJECT=dealer webpack --config webpack/webpack.prod.js --color",
    "build:escape": "cross-env NODE_ENV=production DRVR_PROJECT=tajo webpack --config webpack/webpack.prod.js --color",
    "build:sunshine": "cross-env NODE_ENV=production DRVR_PROJECT=portal webpack --config webpack/webpack.prod.js --color",
    "build:scc-dev": "cross-env DRVR_PROJECT=scc webpack --config webpack/webpack.dev.js --color",
    "build:demo-dev": "cross-env DRVR_PROJECT=demo webpack --config webpack/webpack.dev.js --color",
    "build:dealer-dev": "cross-env DRVR_PROJECT=dealer webpack --config webpack/webpack.dev.js --color",
    "build:escape-dev": "cross-env DRVR_PROJECT=tajo webpack --config webpack/webpack.dev.js --color",
    "build:sunshine-dev": "cross-env DRVR_PROJECT=portal webpack --config webpack/webpack.dev.js --color",
    "postinstall": "npm run setup",
    "setup": "cp ci/pre-commit .git/hooks",
    "dealer": "npm run start:tunnel:dealer",
    "scc": "npm run start:tunnel:scc",
    "demo": "npm run start:tunnel:demo",
    "escape": "npm run start:tunnel:escape",
    "sunshine": "npm run start:tunnel:sunshine",
    "start:tunnel:scc": "cross-env DRVR_PROJECT=scc node server -d",
    "start:tunnel:demo": "cross-env DRVR_PROJECT=demo node server -d",
    "start:tunnel:dealer": "cross-env DRVR_PROJECT=dealer node server -d",
    "start:tunnel:escape": "cross-env DRVR_PROJECT=tajo node server -d",
    "start:tunnel:sunshine": "cross-env DRVR_PROJECT=portal node server -d",
    "pretest": "npm run lint",
    "lintFix": "eslint -c eslintPreCommitConfig.json --quiet --fix",
    "lint": "lint-staged",
    "test": "NODE_PATH=src NODE_ENV=test mocha src/**/specs/*.spec.js"
  },
  "author": "Maksim Orlov, Kirill Bulatsev",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.js": "eslint -c eslintPreCommitConfig.json --quiet --cache"
  },
  "dependencies": {
    "aphrodite": "1.2.4",
    "auth0-js": "9.4.1",
    "babel-polyfill": "6.26.0",
    "billboard.js": "1.3.0",
    "classnames": "2.2.5",
    "clipboard": "1.7.1",
    "compression": "1.7.1",
    "d3": "4.10.2",
    "d3-scale": "1.0.6",
    "d3-shape": "1.2.0",
    "excel-loader": "^1.0.0",
