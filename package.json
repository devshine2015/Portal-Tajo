    "postcss-reporter": "1.3.3",
    "ramda": "0.23.0",
    "react-addons-test-utils": "15.4.2",
    "recompose": "0.19.0",
    "redux-logger": "2.6.1",
    "rimraf": "2.5.2",
    "style-loader": "0.13.1",
    "svg-react-loader": "0.4.0-beta.2",
    "url-loader": "0.5.7"
  }
}
