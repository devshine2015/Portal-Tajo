{
  "name": "installer-feature",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:clean": "rimraf ./build/* ./escape/* ./customer/*",
    "build:clean:escape": "rimraf ./build/* ./escape/*",
    "build:clean:portal": "rimraf ./build/* ./customer/*",
    "build:escape": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.escape.babel.js --color -p",
    "build:portal": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.portal.babel.js --color -p",
    "escape": "npm run start:tunnel:escape",
    "portal": "npm run start:tunnel:portal",
    "start:tunnel:escape": "cross-env NODE_ENV=development DRVR_PORTAL=escape node server",
    "start:tunnel:portal": "cross-env NODE_ENV=development DRVR_PORTAL=portal node server"
  },
  "author": "Maksim Orlov",
  "license": "ISC",
  "babel": {
    "plugins": [
      "transform-class-properties"
    ],
    "presets": [
      "react",
      "stage-0",
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "env": {
      "production": {
        "only": [
          "src"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "compression": "^1.6.2",
    "express": "^4.13.4",
    "file-saver": "^1.3.1",
    "firebase": "^3.0.5",
    "immutable": "^3.8.1",
    "mapbox.js": "^2.4.0",
    "material-ui": "^0.15.0",
    "moment": "^2.13.0",
    "query-string": "^4.2.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.6",
    "redux-thunk": "^2.1.0",
    "sanitize.css": "^3.3.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "ngrok": "^2.1.8",
    "offline-plugin": "^3.2.1",
    "postcss-cssnext": "^2.5.2",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.3.3",
    "recompose": "^0.19.0",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "why-did-you-update": "0.0.8"
  }
}
