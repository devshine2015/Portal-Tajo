{
  "name": "drvr.co",
  "version": "0.0.5",
  "description": "common code base for DRVR portals",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./build ./builds",
    "clean:tajo": "rimraf ./builds/dev/tajo ./builds/prod/tajo",
    "clean:portal": "rimraf ./builds/dev/portal ./builds/prod/portal",
    "clean:ssreports": "rimraf ./builds/dev/ssreports ./builds/prod/ssreports",
    "//": "tasks for local development",
    "build:tajo": "cross-env NODE_ENV=production DRVR_PROJECT=tajo webpack --config webpack/webpack.prod.js --color -p",
    "build:portal": "cross-env NODE_ENV=production DRVR_PROJECT=portal webpack --config webpack/webpack.prod.js --color -p",
    "build:ssreports": "cross-env NODE_ENV=production DRVR_PROJECT=ssreports webpack --config webpack/webpack.prod.js --color -p",
    "build:tajo-dev": "cross-env DRVR_PROJECT=tajo webpack --config webpack/webpack.dev.js --color -d",
    "build:portal-dev": "cross-env DRVR_PROJECT=portal webpack --config webpack/webpack.dev.js --color -d",
    "build:devssreports": "cross-env DRVR_PROJECT=ssreports webpack --config webpack/webpack.dev.js --color -d",
    "tajo": "npm run start:tunnel:tajo",
    "portal": "npm run start:tunnel:portal",
    "ssreports": "npm run start:tunnel:ssreports",
    "start:tunnel:tajo": "cross-env NODE_ENV=development DRVR_PROJECT=tajo node server",
    "start:tunnel:portal": "cross-env NODE_ENV=development DRVR_PROJECT=portal node server",
    "start:tunnel:ssreports": "cross-env NODE_ENV=development DRVR_PROJECT=ssreports node server"
  },
  "author": "Maksim Orlov, Kirill Bulatsev",
  "license": "ISC",
  "babel": {
    "plugins": [
      "transform-class-properties"
    ],
    "presets": [
      "react",
      "stage-0",
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "env": {
      "production": {
        "only": [
          "src"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "compression": "^1.6.2",
    "d3-scale": "^1.0.3",
    "d3-shape": "^1.0.3",
    "express": "^4.13.4",
    "file-saver": "^1.3.1",
    "firebase": "^3.0.5",
    "immutable": "^3.8.1",
    "leaflet-draw": "^0.3.2",
    "leaflet.heat": "^0.2.0",
    "mapbox.js": "^2.4.0",
    "moment": "^2.13.0",
    "query-string": "^4.2.3",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.8",
    "redux-thunk": "^2.1.0",
    "sanitize.css": "^3.3.0",
    "tinycolor2": "^1.4.1",
    "velocity-react": "^1.1.5",
    "webpack": "^2.1.0-beta.21",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "material-ui": "^0.15.4",
    "offline-plugin": "^3.2.1",
    "postcss-cssnext": "^2.5.2",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.3.3",
    "recompose": "^0.19.0",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "svg-react-loader": "^0.3.7",
    "url-loader": "^0.5.7"
  }
}
