{
  "name": "drvr.co",
  "version": "1.2.12",
  "description": "common code base for DRVR portals",
  "repository": {
    "type": "git",
    "url": "https://github.com/drvrteam/portal-tajo.git"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./build ./builds",
    "clean:escape": "rimraf ./builds/dev/tajo ./builds/prod/tajo",
    "clean:sunshine": "rimraf ./builds/dev/portal ./builds/prod/portal",
    "precommit": "npm test",
    "commit": "git-cz",
    "build:escape": "cross-env NODE_ENV=production DRVR_PROJECT=tajo webpack --config webpack/webpack.prod.js --color -p",
    "build:sunshine": "cross-env NODE_ENV=production DRVR_PROJECT=portal webpack --config webpack/webpack.prod.js --color -p",
    "build:escape-dev": "cross-env DRVR_PROJECT=tajo webpack --config webpack/webpack.dev.js --color",
    "build:sunshine-dev": "cross-env DRVR_PROJECT=portal webpack --config webpack/webpack.dev.js --color",
    "//": "run test with config from './test/mocha.opts'",
    "postinstall": "npm run setup",
    "setup": "cp ci/pre-commit .git/hooks",
    "shrink": "npm shrinkwrap --dev",
    "escape": "npm run start:tunnel:escape",
    "sunshine": "npm run start:tunnel:sunshine",
    "start:tunnel:escape": "cross-env DRVR_PROJECT=tajo node server -d",
    "start:tunnel:sunshine": "cross-env DRVR_PROJECT=portal node server -d",
    "test": "NODE_PATH=src NODE_ENV=test mocha"
  },
  "author": "Maksim Orlov, Kirill Bulatsev",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "aphrodite": "1.0.0",
    "babel-polyfill": "6.13.0",
    "compression": "1.6.2",
    "d3-scale": "1.0.3",
    "d3-shape": "1.0.3",
    "express": "4.13.4",
    "file-saver": "1.3.1",
    "firebase": "3.0.5",
    "font-awesome": "4.7.0",
    "google-maps-api": "2.0.1",
    "immutable": "3.8.1",
    "leaflet-contextmenu": "1.2.1",
    "leaflet-draw": "0.3.2",
    "leaflet.heat": "0.2.0",
    "mapbox.js": "2.4.0",
    "material-ui": "0.16.7",
    "moment": "2.13.0",
    "node-polyglot": "2.2.2",
    "node-uuid": "1.4.7",
    "object-path": "0.11.3",
    "query-string": "4.2.3",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "4.4.5",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.5",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-immutable": "3.0.8",
    "redux-thunk": "2.1.0",
    "sanitize.css": "3.3.0",
    "tinycolor2": "1.4.1",
    "velocity-react": "1.1.5",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-react-constant-elements": "6.8.0",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "classnames": "2.2.5",
    "commitizen": "2.9.5",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "cz-conventional-changelog": "1.2.0",
    "enzyme": "2.7.1",
    "eslint": "2.8.0",
    "eslint-config-airbnb": "7.0.0",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "0.6.2",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-react": "4.3.0",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.8.5",
