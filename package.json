{
  "name": "drvr.co",
  "version": "1.4.98",
  "description": "common code base for DRVR portals",
  "repository": {
    "type": "git",
    "url": "https://github.com/drvrteam/portal-tajo.git"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./build ./builds",
    "precz": "npm test",
    "cz": "git-cz",
    "build:dealer": "cross-env NODE_ENV=production DRVR_PROJECT=dealer webpack --config webpack/webpack.prod.js --color",
    "build:escape": "cross-env NODE_ENV=production DRVR_PROJECT=tajo webpack --config webpack/webpack.prod.js --color",
    "build:sunshine": "cross-env NODE_ENV=production DRVR_PROJECT=portal webpack --config webpack/webpack.prod.js --color",
    "build:dealer-dev": "cross-env DRVR_PROJECT=dealer webpack --config webpack/webpack.dev.js --color",
    "build:escape-dev": "cross-env DRVR_PROJECT=tajo webpack --config webpack/webpack.dev.js --color",
    "build:sunshine-dev": "cross-env DRVR_PROJECT=portal webpack --config webpack/webpack.dev.js --color",
    "postinstall": "npm run setup",
    "setup": "cp ci/pre-commit .git/hooks",
    "dealer": "npm run start:tunnel:dealer",
    "escape": "npm run start:tunnel:escape",
    "sunshine": "npm run start:tunnel:sunshine",
    "start:tunnel:dealer": "cross-env DRVR_PROJECT=dealer node server -d",
    "start:tunnel:escape": "cross-env DRVR_PROJECT=tajo node server -d",
    "start:tunnel:sunshine": "cross-env DRVR_PROJECT=portal node server -d",
    "pretest": "npm run lint",
    "lintFix": "eslint -c eslintPreCommitConfig.json --quiet --fix",
    "lint": "lint-staged",
    "test": "NODE_PATH=src NODE_ENV=test mocha src/**/specs/*.spec.js"
  },
  "author": "Maksim Orlov, Kirill Bulatsev",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.js": "eslint -c eslintPreCommitConfig.json --quiet --cache"
  },
  "dependencies": {
    "aphrodite": "1.0.0",
    "auth0-js": "8.8.0",
    "babel-polyfill": "6.13.0",
    "billboard.js": "1.0.1",
    "classnames": "2.2.5",
    "compression": "1.6.2",
    "d3": "4.7.1",
    "d3-scale": "1.0.3",
    "d3-shape": "1.0.3",
    "express": "4.13.4",
    "file-saver": "1.3.1",
    "firebase": "3.0.5",
    "font-awesome": "4.7.0",
    "fuse.js": "3.0.4",
    "google-maps-api": "2.0.1",
    "history": "^3.0.0",
    "immutable": "3.8.1",
    "jspdf": "^1.3.3",
    "jspdf-autotable": "^2.3.2",
    "jwt-decode": "2.2.0",
    "leaflet-ant-path": "0.5.1",
    "leaflet-contextmenu": "1.4.0",
    "leaflet-draw": "0.4.9",
    "leaflet.heat": "0.2.0",
    "mapbox.js": "3.0.1",
    "material-ui": "0.19",
    "moment": "2.13.0",
    "node-polyglot": "2.2.2",
    "node-uuid": "1.4.7",
    "prop-types": "15.5.10",
    "query-string": "4.2.3",
    "ramda": "0.23.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-native-storage": "0.2.2",
    "react-redux": "4.4.5",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.5",
    "react-tap-event-plugin": "2.0.1",
    "recompose": "0.23.1",
    "redux": "3.6.0",
    "redux-immutable": "3.0.8",
    "redux-thunk": "2.1.0",
    "reselect": "3.0.0",
    "sanitize.css": "3.3.0",
    "tinycolor2": "1.4.1",
    "velocity-react": "1.3.3",
    "velocity-animate": "1.5.0",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-loader": "6.4.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-constant-elements": "6.8.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.3.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.4.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.22.0",
    "babel-preset-stage-1": "6.24.1",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "commitizen": "2.9.5",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "cz-conventional-changelog": "1.2.0",
    "enzyme": "2.7.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-mocha": "4.9.0",
    "eslint-plugin-react": "6.10.3",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.8.5",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.17.0",
    "imports-loader": "0.6.5",
    "jsdom": "9.10.0",
    "lint-staged": "4.0.4",
    "mocha": "3.2.0",
    "postcss-cssnext": "2.5.2",
    "postcss-focus": "1.0.0",
    "postcss-loader": "0.9.1",
    "postcss-reporter": "1.3.3",
    "react-addons-test-utils": "15.4.2",
    "redux-logger": "2.6.1",
    "rimraf": "2.5.2",
    "style-loader": "0.13.1",
    "svg-react-loader": "0.4.0-beta.2",
    "url-loader": "0.5.7"
  }
}
